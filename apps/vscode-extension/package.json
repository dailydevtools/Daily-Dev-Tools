{
  "name": "dailydevtools-vscode",
  "displayName": "DailyDevTools",
  "description": "90+ free online developer tools directly in VS Code. Zero context switching.",
  "version": "0.0.1",
  "publisher": "dailydevtools",
  "icon": "icon.png",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/dailydevtools/Daily-Dev-Tools"
  },
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "dailydevtools.openDashboard",
        "title": "DailyDevTools: Open Dashboard",
        "category": "DailyDevTools"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "dailydevtools.openDashboard",
          "group": "modification"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.90.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "glob": "^10.4.1",
    "mocha": "^10.4.0",
    "typescript": "^5.4.5",
    "@vscode/test-electron": "^2.4.0"
  }
}
